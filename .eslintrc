{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"linebreak-style": ["error", "windows"],
		"semi": "off",
		"no-tabs": "off",
		"indent": "off",
		"comma-dangle": "off",
		"arrow-parens": "off",
		"require-jsdoc": "off"
	},
	"extends": ["eslint:recommended", "google"]
}